<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd" version="2.0">
    <managed-bean>
        <managed-bean-name>pages</managed-bean-name>
        <managed-bean-class>org.richfaces.example.Pages</managed-bean-class>
        <managed-bean-scope>application</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>content</managed-bean-name>
        <managed-bean-class>org.richfaces.example.ContentBean</managed-bean-class>
        <managed-bean-scope>none</managed-bean-scope>
    </managed-bean>
</faces-config>
